{
    "name": "myserver",
    "version": 2,
    "alias": "myservernode",
    "builds": [{ "src": "app.js", "use": "@now/node-server" }],
    "routes": [
        {
          "src": "/",
          "dest": "/app.js",
          "methods": ["GET"]
        },
        {
          "src": "/users",
          "dest": "/app.js",
          "methods": ["GET"]
        }
      ]
  }